<equipment>
 <username>Minikappa</username>
 <command>
   <type>spec</type>
   <version>falc141:spec</version>
   <name>Initialize</name>
   <toexecute>minikappa_init</toexecute>
 </command>
 <command>
   <type>spec</type>
   <version>falc141:spec</version>
   <name>Manual Load</name>
   <toexecute>minikappa_manualload</toexecute>
 </command>
 <command>
   <type>spec</type>
   <version>falc141:spec</version>
   <name>Home</name>
   <toexecute>minikappa_home</toexecute>
 </command>
 <specversion>falc141:spec</specversion>
 <command type="spec" name="KappaOn">SCKappaOn</command>
 <command type="spec" name="KappaOff">SCKappaOff</command>
 <channel type="spec" name="KappaInUse">KAPPA_IN_USE</channel>
 <motors>
   <device hwrid="/omega" role="omega"/>
   <device hwrid="/kappa" role="kappa"/>
   <device hwrid="/phi" role="phi"/>
   <device hwrid="/sampx" role="-X"/>
   <device hwrid="/sampy" role="Y"/>
   <device hwrid="/phiy" role="Z"/>
 </motors>
</equipment>
