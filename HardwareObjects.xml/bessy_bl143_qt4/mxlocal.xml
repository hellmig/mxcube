<?xml version="1.0" encoding="iso-8859-1"?>
<beamline_pars>
  <COLLECT_PARS>
    <!--
    <display>adxv</display>
    <adxv_notify_file>/141dat/adxv_notify_status</adxv_notify_file>
    -->
    <escan_dir>/tmp</escan_dir>
    <escan_prefix>escan_</escan_prefix>
    <fileinfo>
      <directory></directory>
      <prefix></prefix>
      <run_number>1</run_number>
    </fileinfo>
    <current_filename></current_filename>
    <dark>1</dark>
  </COLLECT_PARS>
  <SPEC_PARS>
    <phi>phi</phi>
    <wavmot>monoang</wavmot>
    <emot>monoen</emot>
    <detdistmot>distccd</detdistmot>
    <beam>
      <ax>0.000001</ax>
      <ay>0.000001</ay>
      <bx>215.000</bx>
      <by>210.528</by>
    </beam>
      <beamsize>
      <!-- determined by apertures -->
      <max_hor>0.200</max_hor>
      <max_ver>0.200</max_ver>
    </beamsize>
    <detector>
      <binning>1</binning>
      <TangoDeviceName>bl143/ccd/1</TangoDeviceName>
      <LimaDeviceName>bl143/ccd/1</LimaDeviceName>
      <save_raw>0</save_raw>
      <no_xform>0</no_xform>
      <UseDefaultDarkTime>1</UseDefaultDarkTime>
    </detector>
  </SPEC_PARS>
  <BCM_PARS>
    <detector>
      <manufacturer>dectris</manufacturer>
      <type>pilatus</type>
      <model>6m</model>
      <binning>1</binning>
      <mode>shutterless</mode>
      <px>0.172</px>
      <py>0.172</py>
      <cansum>yes</cansum>
      <has_shutterless>False</has_shutterless>
    </detector>
    <detector_radius>215.0</detector_radius>
    <pixelsize>172.00</pixelsize>
    <FileSuffix>cbf</FileSuffix>
    <minimum_exposure_time>1.0</minimum_exposure_time>
    <minimum_phi_speed>0.01</minimum_phi_speed>
    <minimum_phi_oscillation>0.01</minimum_phi_oscillation>
    <maximum_phi_speed>100</maximum_phi_speed>
    <maximum_radiation_exposure>4000</maximum_radiation_exposure>
  </BCM_PARS>
  <VALIDATE_PARS>
    <exposure_time>1,100</exposure_time>
    <start_angle>-1000,1000</start_angle>
    <range>0.0,10</range>
    <number_of_passes>1,10</number_of_passes>
    <start_image_number>0,10000</start_image_number>
    <run_number>0,1000</run_number>
    <overlap>-90,0.99</overlap>
    <number_of_images>1,7200</number_of_images>
  </VALIDATE_PARS>
  <EXTRAVALIDATE_PARS>
    <rotation_speed>0.01,100</rotation_speed>
  </EXTRAVALIDATE_PARS>
 <INHOUSE_USERS>
   <proposal>
     <code>mxihr</code>
   </proposal>
   <proposal>
     <code>opid</code>
   </proposal>
 </INHOUSE_USERS>
</beamline_pars>
