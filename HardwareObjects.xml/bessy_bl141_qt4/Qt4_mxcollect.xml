  <!-- new -->
  <object href="/beam_info" role="beam_info"/>
  <object href="/mach" role="machine_current"/>
  <!-- new -->
  <object href="/pilatus" role="detector"/>
  <object href="/energy" role="energy"/>
  <object href="/resolution" role="resolution"/>
  <object href="/escfilters" role="transmission"/>
