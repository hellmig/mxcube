    <object href="/auto-processing" role="auto_processing"/>
  -->
  <object href="/lims-client-mockup" role="dbserver"/> 
  <object href="/cats" role="sample_changer"/>
  <object href="/Qt4_graphics-manager" role="graphics_manager"/>

